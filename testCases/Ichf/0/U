FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type            pressureInletOutletVelocity;
        value          uniform (0 0 0);

    }
    sides
    {
          type            zeroGradient;
    }
    base
    {
        type            outletInlet;
        value           uniform (0 0 0);
        outletValue     uniform (0 0 0);
    }
    inlet
    {
        type            fixedValue;
        value           uniform (0 0.05 0);
    }
    frontBack
    {
        type        empty;
    }
}

