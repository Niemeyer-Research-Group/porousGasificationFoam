heatTransferModel constCONV;
constHTC false;

Parameters
{
h           2.0;    // era 5
SAV         236;    // SAV_border=6*l^2/l^3=6/l=6/0.0254=236
cylinderRadius  0.005;  // for h = Nud k_f / (2 pipeRadis) = 2 * 2e-5*1e+3 * 1 / 0.005 -> 4.
poreRadius  0.005;  // for SAV = 2*sqrt( \epsilon_0 \esilon ) / pipeRadius -> 200
}


