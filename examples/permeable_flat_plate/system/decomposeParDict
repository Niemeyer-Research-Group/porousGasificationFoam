FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 3;

preservePatches (inlet_porous outlet_porous);

method            simple;

simpleCoeffs
{
    n               (1 3 1);
    delta           0.001;
}

